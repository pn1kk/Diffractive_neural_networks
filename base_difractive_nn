   "execution_count": 71,
   "id": "91d1e9b5-8703-4201-8743-b54eac91da99",
   "metadata": {},
   "outputs": [],
   "source": [
    "NUMBER_OF_CLASSES = 10  # TODO: how many classes do we have?\n",
    "NEURON_SIZE = neuron_size"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "8d1dabde-317c-45c3-a9ee-4bffd403b5cb",
   "metadata": {},
   "source": [
    "### <span style=\"color:red\">3.2.1. Detector mask (TODO)</span>"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 72,
   "id": "e8c1e018-fb72-480e-b7a4-5eaf29145ed3",
   "metadata": {},
   "outputs": [],
   "source": [
    "detector_segment_size_m = 6.4 * working_wavelength  # in [m]\n",
    "DETECTOR_SEGMENT_SIZE = int(detector_segment_size_m / neuron_size) # in neurons (int)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 73,
   "id": "7c8aa135-9ddb-41c2-89d3-bf25d37aa04b",
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_detector_mask(\n",
    "    segment_size_in_neurons,  # int value\n",
    "    order_of_digits=list(range(NUMBER_OF_CLASSES))\n",
    "):\n",
    "    for i in range(NUMBER_OF_CLASSES):  # check if all places for digits specified\n",
    "        if not i in order_of_digits:\n",
    "            print('Wrong ordering list!')\n",
    "\n",
    "    if not len(order_of_digits) == NUMBER_OF_CLASSES:\n",
    "        print('Wrong ordering list!')\n",
    "\n",
    "    x_grid, y_grid = SIM_PARAMS.meshgrid(x_axis='W', y_axis='H')\n",
    "\n",
    "    mask_det = torch.ones(size = ALL_SIZE)*(-1)\n",
    "    \n",
    "    for i in range(3):\n",
    "        select = (\n",
    "            (x_grid < (-1.5 + 2 * i) * detector_segment_size_m) & (x_grid > (-2.5 + 2 * i) * detector_segment_size_m) & \n",
    "            (y_grid < (-1.5) * detector_segment_size_m) & (y_grid > (-2.5) * detector_segment_size_m)\n",
    "        )\n",
    "    \n",
    "        mask_det[select] = order_of_digits[i]\n",
    "    \n",
    "    for i in range(4):\n",
    "        select = (\n",
    "            (x_grid < (-2.5 + 2 * i) * detector_segment_size_m) & (x_grid > (-3.5 + 2 * i) * detector_segment_size_m) & \n",
    "            (y_grid < 0.5 * detector_segment_size_m) & (y_grid > -0.5 * detector_segment_size_m)\n",
    "        )\n",
    "    \n",
    "        mask_det[select] = order_of_digits[i+3]\n",
    "    \n",
    "    for i in range(3):\n",
    "        select = (\n",
    "            (x_grid < (-1.5 + 2 * i) * detector_segment_size_m) & (x_grid > (-2.5 + 2 * i) * detector_segment_size_m) & \n",
    "            (y_grid < (-1.5 + 4) * detector_segment_size_m) & (y_grid > (-2.5 + 4) * detector_segment_size_m)\n",
    "        )\n",
    "    \n",
    "        mask_det[select] = order_of_digits[i + 7]\n",
    "\n",
    "    return mask_det"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "31171243-b32c-4261-b850-4c0a6cd5deb8",
   "metadata": {},
   "outputs": [],
   "source": []
